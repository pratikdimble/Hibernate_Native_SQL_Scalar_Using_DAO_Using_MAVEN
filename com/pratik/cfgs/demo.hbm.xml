<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.pratik.domain.demo" table="product">
			<id name="pid" column="PRODID"/>
			<property name="pname" column="PRODNAME"/>
			<property name="p_price" column="PRICE"/>
			<property name="p_quantity" column="quantity"/>

   		 </class>
    </hibernate-mapping>
